        <elements xsi:type="org.nasdanika.vinci.app:Action" title="Связывание нескольких моделей  единую федерацию" description="создание сайта, в котором используются различные макеты страницы" id="03c1da59-db49-4da6-b49d-ea4692c36884" text="Связывание нескольких моделей  единую федерацию"/>
        <elements xsi:type="org.nasdanika.vinci.app:Action" title="Создание различных макетов страниц в рамках одного сайта" id="856d41fa-c475-4277-b4eb-7c4eed42f9b0" text="Создание различных макетов страниц в рамках одного сайта"/>
        <elements xsi:type="org.nasdanika.vinci.app:Action" title="Оформление элементов сайта" id="c89e9735-3a44-4144-96e7-4b86011085d8" text="Оформление элементов сайта"/>
